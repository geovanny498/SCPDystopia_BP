{
    "format_version": "1.12.0",
    "minecraft:entity": {
        "description": {
            "identifier": "lc:dt_rpg_rocket",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false,
            "runtime_identifier": "minecraft:snowball"
        },
        "component_groups": {
            "minecraft:exploding": {
                "minecraft:explode": {
                    "fuse_length": 0,
                    "fuse_lit": true,
                    "power": 5.0,
                    "causesFire": true,
                    "fire_affected_by_griefing": true,
                    "destroy_affected_by_griefing": true
                }
            },
            "despawning": {
                "minecraft:despawn": {}
            } 
        },
        "components": {
            "minecraft:tick_world": {
				"distance_to_players": 128,
				"never_despawn": false,
				"radius": 2
			},
            "minecraft:type_family": {
                "family": [ "rocket", "projectile" ]
            },
            "minecraft:collision_box": {
                "width": 0.4,
                "height": 0.4
            },
            "minecraft:projectile": {
                "on_hit": {
                    "definition_event": {
                        "affect_projectile": true,
                        "eventTrigger": {
                            "event": "minecraft:explode",
                            "target": "self"
                        }
                    }
                },
                "liquid_inertia": 1.0,
                "hit_sound": "gun.hit",
                "power": 10.0,
                "gravity": 0.06,
                "inertia": 1.2,
                "uncertainty_base": 3,
                "uncertainty_multiplier": 2,
                "anchor": 1,
                "offset": [
                    0,
                    -0.1,
                    0
                ],
                "reflect_on_hurt": false,
                "catch_fire": true
            },
            "minecraft:physics": {},
            "minecraft:timer": {
                "looping": false,
                "time": 0.64,
                "time_down_event": {
                    "event": "minecraft:explode"
                }
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 80.0,
                    "max_dropped_ticks": 7,
                    "use_motion_prediction_hints": true
                }
            }
        },
        "events": {
            "minecraft:explode": {
                "add": {
                    "component_groups": [
                        "minecraft:exploding"
                    ]
                }
            },
            "minecraft:start_despawn": {
                "add": {
                    "component_groups": [
                        "despawning"
                    ]
                }
            }
        }
    }
}