                "liquid_inertia": 1.0,
                "hit_sound": "gun.hit",
                "power": 15.0,
                "gravity": 0.0,
                "inertia": 1.0,
                "uncertainty_base": 3.5,
                "uncertainty_multiplier": 1.5,
                "anchor": 1,
                "offset": [
                    0,
                    -0.1,
                    0
                ],
                "reflect_on_hurt": false,
                "catch_fire": false
            },
            "minecraft:physics": {},
            "minecraft:timer": {
                "looping": false,
                "time": 0.427,
                "time_down_event": {
                    "event": "minecraft:start_despawn"
                }
            },
            "minecraft:conditional_bandwidth_optimization": {
                "default_values": {
                    "max_optimized_distance": 80.0,
                    "max_dropped_ticks": 7,
                    "use_motion_prediction_hints": true
                }
            }
        },
        "events": {
            "minecraft:explode": {
                "add": {
                    "component_groups": [
                        "minecraft:exploding"
                    ]
                }
            },
            "minecraft:start_despawn": {
                "add": {
                    "component_groups": [
                        "despawning"
                    ]
                }
            }
        }
