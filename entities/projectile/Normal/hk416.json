{
    "format_version": "1.12.0",
    "minecraft:entity": {
        "description": {
            "identifier": "lc:dt_hk416_bullet",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false,
            "runtime_identifier": "minecraft:snowball"
        },
        "component_groups": {
            "minecraft:exploding": {
                "minecraft:explode": {
                    "fuse_length": 0,
                    "fuse_lit": true,
                    "power": 0.35,
                    "causes_fire": false,
                    "fire_affected_by_griefing": true,
                    "destroy_affected_by_griefing": true
                }
            },
            "despawning": {
                "minecraft:despawn": {}
            } 
        },
        "components": {
            "minecraft:tick_world": {
				"distance_to_players": 128,
				"never_despawn": false,
				"radius": 2
			},
            "minecraft:type_family": {
                "family": [
                    "bullet",
                    "projectile"
                ]
            },
            "minecraft:break_blocks": {
                "breakable_blocks": [
                    "glass",
                    "glass_pane",
                    "stained_glass",
                    "stained_glass_pane",
                    "standing_sign",
                    "vine",
                    "wall_banner",
                    "wall_sign",
                    "web",
                    "wheat"
                ]
            },
            "minecraft:spawn_entity": {
                "entities": {
                    "min_wait_time": 0,
                    "max_wait_time": 0,
                    "single_use": true,
                    "spawn_entity": "lc:dt_none",
                    "spawn_sound": "purr",
                    "num_to_spawn": 1
                }
            },
            "minecraft:collision_box": {
                "width": 0.25,
                "height": 0.25
            },
            "minecraft:projectile": {
                "on_hit": {

                      
                    "impact_damage": {
                        "damage": 0,
                        /*"knockback": true,*/
                        "semi_random_diff_damage": false,
                        "destroy_on_hit": false
                    },
                      
                    "stick_in_ground": {
                        "shake_time": 0.1
                    },
                    "definition_event": {
                        "affect_projectile": true,
                        "event_trigger": {}
                    }
